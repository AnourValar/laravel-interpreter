{
    "name": "anourvalar/laravel-interpreter",
    "description": "Laravel's locales translate through the single (flat) file",
    "keywords": ["anourvalar", "laravel", "locale", "trans", "localization", "translate", "export", "import", "i18"],
    "homepage": "https://github.com/AnourValar/laravel-interpreter",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "laravel/framework": "^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.0",
        "friendsofphp/php-cs-fixer": "^3.26",
        "squizlabs/php_codesniffer": "^3.7",
        "psalm/plugin-laravel": "^3.0"
    },
    "autoload": {
        "psr-4": {"AnourValar\\LaravelInterpreter\\": "src/"}
    },
    "extra": {
        "laravel": {
            "providers": [
                "AnourValar\\LaravelInterpreter\\Providers\\LaravelInterpreterServiceProvider"
            ]
        }
    }
}
